Options +FollowSymLinks 

RewriteEngine On



# Rewrite for index.php,cars.php,...

RewriteRule ^(home)$ index.php?page_name=$1
RewriteRule ^(jewelry)$ jewelry.php?page_name=$1
RewriteRule ^(dprojects)$ dproject.php?page_name=$1
RewriteRule ^(makeups)$ makeup.php?page_name=$1
RewriteRule ^(dmakeups)$ dmakeup.php?page_name=$1
RewriteRule ^(manages)$ manage.php?page_name=$1
RewriteRule ^(clients)$ client.php?page_name=$1
RewriteRule ^(gallery)$ gallary.php?page_name=$1
RewriteRule ^(pages)$ page.php?page_name=$1
RewriteRule ^(cpages)$ cpage.php?page_name=$1
RewriteRule ^(contacts)$ contact.php?page_name=$1 



# Rewrite for single variables

RewriteRule ^page-([^/]+)$ page.php?page_name=$1
RewriteRule ^jewelrys-([^/]+)$ jewelry.php?project_name=$1
RewriteRule ^djewelrys-([^/]+)$ djewelry.php?dproject_name=$1
RewriteRule ^makeups-([^/]+)$ makeup.php?service_name=$1
RewriteRule ^dmakeups-([^/]+)$ dmakeup.php?dservice_name=$1
RewriteRule ^clients-([^/]+)$ client.php?client_name=$1
RewriteRule ^gallery-([^/]+)$ gallary.php?gallery_name=$1
RewriteRule ^pages-([^/]+)$ page.php?pages_name=$1
RewriteRule ^cpages-([^/]+)$ cpage.php?cpage_name=$1



